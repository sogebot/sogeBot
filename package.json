    "@sogebot/ui-public": "^2.0.0",
    "@twurple/api": "^5.0.9",
    "@twurple/chat": "^5.0.9",
    "@twurple/pubsub": "^5.0.9",
    "axios": "0.24.0",
    "basic-auth": "2.0.1",
    "better-sqlite3": "^7.4.5",
    "blocked-at": "1.2.0",
