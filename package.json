{
  "name": "sogebot",
  "version": "7.6.0-SNAPSHOT",
  "description": "Free Twitch Bot built on Node.js",
  "private": true,
  "main": "main.js",
  "author": "Michal Orlik <sogehige@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=7.0"
  },
  "dependencies": {
    "async-request": "1.2.0",
    "auto-load": "3.0.0",
    "axios": "0.18.0",
    "basic-auth": "2.0.0",
    "bootstrap": "4.1.1",
    "bootstrap-menu": "1.0.14",
    "bootstrap-slider": "10.0.2",
    "bootstrap-toggle": "2.2.2",
    "chalk": "2.4.1",
    "chart.js": "2.7.2",
    "compare-versions": "3.3.0",
    "cross-env": "5.2.0",
    "currency-symbol-map": "4.0.4",
    "debug": "3.1.0",
    "decode-html": "2.0.0",
    "express": "4.16.3",
    "figlet": "1.2.0",
    "flat": "4.0.0",
    "flv.js": "1.4.2",
    "glob": "7.1.2",
    "gridstack": "0.4.0",
    "jquery": "3.3.1",
    "lodash": "4.17.10",
    "mathjs": "5.0.0",
    "moment": "2.22.2",
    "moment-precise-range-plugin": "1.3.0",
    "moment-timezone": "0.5.21",
    "mongodb": "3.1.0",
    "mongodb-uri": "0.9.7",
    "nedb": "1.8.0",
    "nedb-promise": "2.0.1",
    "node-hue-api": "2.4.2",
    "node-sass": "4.9.0",
    "npm-run-all": "4.1.3",
    "page": "1.8.6",
    "popper.js": "1.14.3",
    "request": "2.87.0",
    "safe-eval": "0.3.0",
    "snekfetch": "^4.0.4",
    "socket.io": "2.1.1",
    "sortablejs": "1.7.0",
    "spotify-web-api-node": "3.1.1",
    "terminal-kit": "1.17.11",
    "twitch-emoticons": "http://deps.sogehige.tv/twitch-emoticons-2.1.0.tgz",
    "twitch-js": "2.0.0-alpha.20",
    "twitter": "1.7.1",
    "url-join": "4.0.0",
    "velocity-animate": "1.5.1",
    "winston": "3.0.0",
    "winwheel": "1.0.1",
    "xregexp": "4.2.0",
    "yargs": "11.0.0",
    "youtube-search": "1.1.1",
    "ytdl-core": "0.20.4"
  },
  "scripts": {
    "prebuild": "npm install rimraf bestzip npm-run-all cross-var -g",
    "build": "npm-run-all --sequential cleanup:* build:modules build:shrinkwrap build:zip",
    "build:css": "npm-run-all --parallel compile:css:*",
    "build:modules": "npm install",
    "build:shrinkwrap": "npm shrinkwrap",
    "build:zip": "cross-var bestzip sogeBot-$npm_package_version.zip npm-shrinkwrap.json config.example.json libs/ locales/ main.js cluster.js public/ LICENSE package.json README.md AUTHORS tools/ bin/ bat/ scss/ dist/",
    "compile:css:light": "node-sass --output-style expanded --precision 6 scss/themes/light.scss public/dist/css/light.css",
    "compile:css:dark": "node-sass --output-style expanded --precision 6 scss/themes/dark.scss public/dist/css/dark.css",
    "standard": "standard main.js && standard test/* && standard libs/* && standard libs/systems/* && standard libs/overlays/* && standard libs/widgets/*",
    "test": "mocha --recursive --timeout 20000 --exit",
    "test:config:mongodb": "cp config.example.json config.json && sed -i -- 's/nedb/mongodb/g' config.json",
    "test:config:nedb": "cp config.example.json config.json",
    "start": "node ./tools/migrate && node ./tools/migrate --delete && npm-run-all --parallel build:css copy:dist && cross-env NODE_ENV=production node ./main || true",
    "start-no-migration": "npm-run-all --parallel build:css copy:dist && cross-env NODE_ENV=production node ./main || true",
    "debug": "npm run copy:dist && node --inspect ./main || true",
    "cleanup": "node ./tools/cleanup.js || true",
    "cleanup:dist": "rimraf public/dist/bootstrap* public/dist/carousel/* public/dist/gallery/* public/dist/jquery public/dist/lodash public/dist/velocity-animate public/dist/popper.js public/dist/flv.js public/dist/css/dark.css public/dist/css/light.css",
    "cleanup:modules": "rimraf node_modules/",
    "cleanup:shrinkwrap": "rimraf npm-shrinkwrap.json",
    "copy:dist": "node tools/copy-dist-files.js"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "locate-path": "3.0.0",
    "mocha": "5.2.0",
    "sinon": "6.0.1",
    "standard": "11.0.1",
    "test-until": "1.1.1"
  }
}
