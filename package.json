    "axios-mock-adapter": "1.20.0",
    "bestzip": "2.2.0",
    "deepmerge": "4.2.2",
    "docsify": "4.12.1",
    "docsify-cli": "4.4.3",
    "empty": "0.10.1",
    "escope": "3.6.0",
    "eslint": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "git-semver-tags": "4.1.1",
    "jsonlint-mod": "1.7.6",
    "lint-staged": "12.1.2",
    "minimist": "1.2.5",
    "mocha": "9.1.3",
    "nyc": "15.1.0",
    "process": "0.11.10",
    "simple-git-hooks": "^2.7.0",
    "sinon": "12.0.1",
    "test-until": "1.1.1",
    "tsc-alias": "^1.4.1",
    "tsconfig-paths": "^3.12.0",
    "typescript": "4.5.2",
    "url-regex": "5.0.0",
    "util": "0.12.4"
  },
  "lint-staged": {
    "*.js": [
      "npx eslint --fix --quiet"
    ],
    "*.ts": [
      "npx eslint --fix --quiet"
    ],
    "*.json": [
      "npx jsonlint"
    ]
  }
}
