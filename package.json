    "request": "2.88.2",
    "rotating-file-stream": "3.0.2",
    "safe-eval": "0.4.1",
    "sanitize-filename": "1.6.3",
    "shortid": "^2.2.16",
    "socket.io": "4.4.0",
    "socket.io-client": "4.4.0",
    "source-map-support": "0.5.21",
    "spotify-web-api-node": "5.0.2",
    "strip-ansi": "6.0.0",
    "strip-comments": "2.0.1",
    "tiny-typed-emitter": "2.1.0",
