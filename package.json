    "pre-commit": "npx simple-git-hooks && npx lint-staged",
    "commit-msg": "node ./tools/pre-commit-message.js $1 $2 $3",
    "preserveUnused": true
  },
  "main": "main.js",
  "author": "Michal Orlik <sogehige@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@sogebot/ui-admin": "^29.0.0",
